{
  "name": "bcs-seattle",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect' next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "email": "email dev --dir components/email-templates",
    "prettier-fix": "prettier --write .",
    "stripe:login": "stripe login",
    "stripe:listen": "stripe listen --forward-to=localhost:3000/api/webhooks",
    "stripe:fixtures": "stripe fixtures fixtures/stripe-fixtures.json",
    "supabase:start": "pnpm exec supabase start",
    "supabase:stop": "pnpm exec supabase stop",
    "supabase:status": "pnpm exec supabase status",
    "supabase:restart": "pnpm run supabase:stop && pnpm run supabase:start",
    "supabase:reset": "pnpm exec supabase reset",
    "supabase:link": "pnpm exec supabase link",
    "supabase:generate-types": "pnpm exec supabase gen types typescript --local --schema public > types_db.ts",
    "supabase:generate-migration": "pnpm exec supabase db diff | pnpm exec supabase migration new",
    "supabase:generate-seed": "pnpm exec supabase db dump --data-only -f supabase/seed.sql",
    "supabase:push": "pnpm exec supabase push",
    "supabase:pull": "pnpm exec supabase pull",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPattern=integration",
    "test:unit": "jest --testPathPattern=unit"
  },
